{
  "expo": {
    "name": "solace_Project2",
    "slug": "solace_Project2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/notificationFavicon(TEST).png",
    "scheme": "solaceproject2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#7267D9",
        "foregroundImage": "./assets/images/notificationFavicon(TEST).png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/notificationFavicon(TEST).png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/notificationFavicon(test).svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notificationFavicon(TEST).png",
          "color": "#7267D9",
          "sounds": []
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
